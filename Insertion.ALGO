
ALGORITHM INSERTION (var t: table of integer; )
var 
i:INTEGER;
j:INTEGER;
k:INTEGER;
BEGIN
j:=2;
WHILE ( J<=t.length) DO
    i:=j-1;
    k:=t[j];
    WHILE ( i>0 ) and (t[i]>k)) do
      t[i+1]:=t[i];
      i:=i-1;
      END_WHILE;
        t[i+1]:=k;
         j:=j+1;
END_WHILE
END.
